{% extends 'base.html.twig' %}

 {% block title %}Log in!{% endblock %}

 {% block body %}
     <h1>S'inscrire</h1>
     {% for label, messages in app.flashes %}
         {% for message in messages %}
             <div class="alert alert-{{ label }} flash-{{ label }}">
                 {{ message }}
             </div>
         {% endfor %}
     {% endfor %}
     {{ form(form) }}
 {% endblock %}